@InProceedings{Hastings:aied2011,
  author = 		 name:psjsk,
  title = 		 {Text Categorization for Assessing Multiple Documents Integration, or {John Henry} Visits a Data Mine},
  booktitle = {Proceedings of the 15th International Conference on Artificial Intelligence in Education},
  year = 	 2011,
  editor = 	 {G. Biswas and S. Bull},
